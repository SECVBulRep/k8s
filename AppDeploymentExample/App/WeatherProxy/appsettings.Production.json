{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Redis": {
    "ClusterNodes": "redis-master.redis.svc.cluster.local:6379",
    "User": "myapp-user",
    "Password": "myapp-secure-password",
    "KeyPrefix": "myapp"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:80"
      }
    }
  },  
  "Urls": {
    "WeatherApi": "http://api-weather"
  },
  "RabbitMQ": {
    "Host": "rabbitmq-cluster.rabbitmq-cluster.svc.cluster.local",
    "Port": 5672,
    "VirtualHost": "/",
    "Username": "admin",
    "Password": "admin"
  }
}
